<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/gray_background"
    tools:context=".Addplaced">

    <TextView
        android:id="@+id/phone_up1"
        android:layout_width="71dp"
        android:layout_height="50dp"
        android:layout_marginEnd="4dp"
        android:gravity="center"
        android:text="Phone:"
        android:textSize="19sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/phone_p"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.533" />

    <TextView
        android:id="@+id/sdasd"
        android:layout_width="73dp"
        android:layout_height="43dp"
        android:layout_marginTop="252dp"
        android:layout_marginEnd="4dp"
        android:gravity="center"
        android:text="Branch:"
        android:textSize="19sp"
        app:layout_constraintEnd_toStartOf="@+id/branch_p"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/email_up1"
        android:layout_width="93dp"
        android:layout_height="44dp"
        android:layout_marginTop="168dp"
        android:layout_marginEnd="4dp"
        android:gravity="right"
        android:text="Email:"
        android:textSize="19sp"
        app:layout_constraintEnd_toStartOf="@+id/email_p"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/name_up"
        android:layout_width="87dp"
        android:layout_height="50dp"
        android:layout_marginTop="84dp"
        android:layout_marginEnd="8dp"
        android:gravity="right"
        android:text="Name:"
        android:textSize="20sp"
        app:layout_constraintEnd_toStartOf="@+id/stud_name_p"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:id="@+id/stud_name_p"
        android:layout_width="285dp"
        android:layout_height="62dp"
        android:layout_marginStart="92dp"
        android:background="@drawable/rounded_edittext"
        android:ems="10"
        android:foregroundGravity="center"
        android:gravity="center"
        android:inputType="textPersonName"
        android:textColorHint="#708090"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.125" />

    <EditText
        android:id="@+id/email_p"
        android:layout_width="285dp"
        android:layout_height="62dp"
        android:layout_marginStart="92dp"
        android:background="@drawable/rounded_edittext"
        android:ems="10"
        android:foregroundGravity="center"
        android:gravity="center"
        android:inputType="textPersonName"
        android:textColorHint="#708090"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.254" />

    <EditText
        android:id="@+id/branch_p"
        android:layout_width="285dp"
        android:layout_height="62dp"
        android:layout_marginStart="92dp"
        android:background="@drawable/rounded_edittext"
        android:ems="10"
        android:foregroundGravity="center"
        android:gravity="center"
        android:inputType="textPersonName"
        android:textColorHint="#708090"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.405" />

    <EditText
        android:id="@+id/phone_p"
        android:layout_width="285dp"
        android:layout_height="62dp"
        android:layout_marginStart="92dp"
        android:background="@drawable/rounded_edittext"
        android:ems="10"
        android:foregroundGravity="center"
        android:gravity="center"
        android:inputType="textPersonName"
        android:textColorHint="#708090"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.542"
        tools:ignore="SpeakableTextPresentCheck" />

    <Button
        android:id="@+id/addata"
        android:layout_width="130dp"
        android:layout_height="52dp"
        android:layout_gravity="center"
        android:text="Add Data"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.498"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.758" />

</androidx.constraintlayout.widget.ConstraintLayout>